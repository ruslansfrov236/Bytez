
@model List<ContactWall>


<div class="col-lg-12 grid-margin stretch-card">
    <div class="card">
        <div class="card-body">
            <h4 class="card-title">Create contactWall</h4>
            <a class="card-description" style="display:block;  margin:20px 0" href="/admin/contactWall/create">
                Add contactWall
            </a>
            <table class="table table-dark">
                <thead>
                    <tr>

                        <th> Name </th>
                        <th> Count </th>
                        <th> Create Date </th>
                        <th> Update Date </th>

                        <th> Update  </th>

                        <th> Delete  </th>
                    </tr>
                </thead>
                <tbody>
                    @{
                        foreach (var contactWall in Model)
                        {
                            <tr>


                                <td> @contactWall.Title  </td>
                                <td>  @contactWall.Description </td>
                                <td> @contactWall.CreatedDate</td>
                                <td> @contactWall.UpdatedDate</td>

                                <td>
                                    <a href="/admin/contactWall/update/@contactWall.Id">
                                        Update
                                    </a>
                                </td>

                                <td>
                                    <form method="post" asp-controller="contactWall" asp-action="Delete" asp-area="Admin" asp-route-id="@contactWall.Id">

                                        <button type="submit" class="btn btn-link"> Delete</button>
                                    </form>
                                </td>

                            </tr>
                        }
                    }


                </tbody>
            </table>
        </div>
    </div>
</div>