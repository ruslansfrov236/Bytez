@model List<Message>


<div class="col-lg-12 grid-margin stretch-card">
    <div class="card">
        <div class="card-body">
            <h4 class="card-title">Create email</h4>
            
            <table class="table table-dark">
                <thead>
                    <tr>

                        <th> Name </th>

                        <th> Create Date </th>
                  
                        <th> Details  </th>
                        
                    </tr>
                </thead>
                <tbody>
                    @{
                        foreach (var email in Model)
                        {
                            <tr>


                                <td> @email.Name  </td>

                                <td> @email.CreatedDate</td>
                              

                                <td> <a href="/admin/email/details/@email.Id">Details </a></td>
                                

                               

                            </tr>
                        }
                    }


                </tbody>
            </table>
        </div>
    </div>
</div>